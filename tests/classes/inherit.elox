
class PopulatedArea {
    init(name, population) {
        this.name = name;
        this.population = population;
    }

    name() {
        return this.name;
    }

    population() {
        return this.population;
    }

    format() {
        return this.name + " has " + this.population + " inhabitants";
    }
}

